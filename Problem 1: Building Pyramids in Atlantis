#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;  // Read the height of the pyramid

    if(n >= 1 && n <= 100)
    {
        for (int i = 1; i <= n; ++i) {
        // Print asterisks
        for (int k = 1; k <= i; ++k) {
            cout << '*';
        }
        // Move to the next line
        cout << endl;
    }
    }
    else
    {
        cout << "invalid";
    }

    return 0;
}
