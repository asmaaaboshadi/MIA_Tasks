#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, target;
    cin >> n;  // Read the number of elements in the array

    vector<int> array(n);

    // Read the elements of the array
    for (int i = 0; i < n; ++i) {
        cin >> array[i];
    }

    cin >> target;  // Read the target value

    // Search for the target in the array
    int index = -1; // remain the same if the number doesn't exist
    for (int i = 0; i < n; ++i) {
        if (array[i] == target) {
            index = i;
            break;
        }
    }

    // Output the index of the target
    cout << index << endl;

    return 0;
}
   
